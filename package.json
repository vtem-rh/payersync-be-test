{
  "name": "payersync-onboarder-backend",
  "version": "0.1.0",
  "bin": {
    "payersync-onboarder-backend": "bin/payersync-onboarder-backend.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:integration": "jest test/adyen-onboarding-handler.integration.test.ts",
    "test:update": "jest -u",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "cdk": "cdk",
    "deploy:windows": "node -e \"require('dotenv').config(); process.env.APP_NAME = process.env.APP_NAME || 'payersyncOnboarder'; require('child_process').execSync('cdk deploy --context environment=' + (process.argv[2] || 'dev'), {stdio: 'inherit', env: process.env})\"",
    "deploy": "node -e \"require('dotenv').config(); require('child_process').execSync('APP_NAME=' + (process.env.APP_NAME || 'payersyncOnboarder') + ' cdk deploy --context environment=' + process.argv[1], {stdio: 'inherit'})\"",
    "destroy": "node -e \"require('dotenv').config(); require('child_process').execSync('APP_NAME=' + (process.env.APP_NAME || 'payersyncOnboarder') + ' cdk destroy --force --context environment=' + process.argv[1], {stdio: 'inherit'})\"",
    "outputs": "node -e \"require('dotenv').config(); require('child_process').execSync('APP_NAME=' + (process.env.APP_NAME || 'payersyncOnboarder') + ' cdk deploy --outputs-file cdk-outputs.json --context environment=' + process.argv[1], {stdio: 'inherit'})\"",
    "deploy:hotswap": "node -e \"require('dotenv').config(); require('child_process').execSync('APP_NAME=' + (process.env.APP_NAME || 'payersyncOnboarder') + ' cdk deploy --hotswap --context environment=' + process.argv[1], {stdio: 'inherit'})\"",
    "watch:cdk": "cdk watch --context environment=dev"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.7.9",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "aws-cdk": "2.1019.1",
    "aws-sdk-client-mock": "^4.1.0",
    "aws-sdk-client-mock-jest": "^4.1.0",
    "cdk-nag": "^2.0.0",
    "dotenv": "^16.6.1",
    "esbuild": "^0.25.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.828.0",
    "@aws-sdk/client-eventbridge": "^3.828.0",
    "@aws-sdk/client-lambda": "^3.859.0",
    "@aws-sdk/client-secrets-manager": "^3.828.0",
    "@aws-sdk/client-sns": "^3.828.0",
    "@aws-sdk/client-ssm": "^3.0.0",
    "@aws-sdk/client-s3": "^3.828.0",
    "@smithy/service-error-classification": "^2.1.3",
    "@types/aws-lambda": "^8.10.0",
    "@types/uuid": "^9.0.8",
    "@types/pg": "^8.11.10",
    "aws-cdk-lib": "^2.201.0",
    "aws-xray-sdk-core": "^3.10.3",
    "axios": "^1.11.0",
    "constructs": "^10.0.0",
    "uuid": "^9.0.1",
    "pg": "^8.13.1",
    "zod": "^3.25.67"
  },
  "engines": {
    "node": "24.2.0",
    "npm": "11.3.0"
  }
}
